; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VR Capture"
run/main_scene="res://splash_screen/splash_screen_manager.tscn"
config/features=PackedStringArray("4.6", "Mobile")

[autoload]

Auth="*res://addons/manch_auth/auth.gd"
SyncManager="*res://capture/sync_manager.gd"

[display]

window/size/viewport_width=2048
window/size/viewport_height=1536

[rendering]

textures/vram_compression/import_etc2_astc=true

[xr]

openxr/enabled=true
shaders/enabled=true
